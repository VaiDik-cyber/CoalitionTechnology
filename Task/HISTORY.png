<script>
  var psd_filename = '';
  var psd_converting = false;

  convertsuccess = function (data) {
    console.log('convertsuccess ');
    console.log('data :' + JSON.stringify(data));
    $("#progressbar").progressbar({ value: 100});
    var form = $('<form></form>');
    $(form).hide().attr('method','post').attr('action','download.php');
    for (i in data) {
      var input = $('<input type="hidden" />').attr('name',i).val(data[i]);
      $(form).append(input);
    }
    $(form).appendTo('body').submit();
  };

  status_log = function (data) {
    if(data.success == false) {
      psd_converting = false;
      return;
    }
    $('ul.qq-upload-list').css('display','none');
    console.log(' status log ');
    console.log('data :'+JSON.stringify(data));
    console.log('Current pos :'+data.current);
    cur_pos = data.current;
    $("#progressbar").progressbar({ value: cur_pos});
    return;
  };

  fetch_status = function () {
    $.post( "status.php",  { 'filename': psd_filename}, function(data) { status_log(data);},"json");
    if(psd_converting) {
      t = setTimeout("fetch_status()", 1000);
    }
  };

  function createUploader() {
    var uploader = new qq.FileUploader({
      element: document.getElementById('file-uploader'),
      action: 'upload.php',
      debug: true,
      onProgress: function(id, fileName, loaded, total){
        pos = (loaded/total)*100;
        $('.qq-upload-list').css('display','block');
        $("#progressbar").progressbar({ value: pos});
        $('#progressbar').css('display','block');
        $('.qq-upload-failed-text').css('display','none');
      },
      onCancel: function(){
        $('#progressbar').css('display','none');
        $('.qq-upload-list').css('display','none');
        psd_converting = false;
      },
      onComplete: function(id, fileName, responseJSON) {
        psd_converting = true;
        psd_filename = responseJSON.filename;
        console.log('ID :'+id);
        console.log('fileName :'+fileName);
        console.log('responseJSON :' + JSON.stringify(responseJSON));
        console.log('file name :'+responseJSON.filename);

        console.log('Call convert...');
        $.post( "convert.php", { 'filename': responseJSON.filename}, function(data) { psd_converting = false; convertsuccess(data);},"json");

        if(psd_converting) {
          fetch_status();
          t = setTimeout("fetch_status()", 1000);
          $('ul.qq-upload-list').css('display','none');
          $("#progressbar").append('<div class="progressbar_processing">Processing…</div>');
        }
      },
      // validation
      // ex. ['jpg', 'jpeg', 'png', 'gif'] or []
      allowedExtensions: ['psd'],
      // each file size limit in bytes
      // this option isn't supported in all browsers
      sizeLimit: 100000000, // max size
      minSizeLimit: 0 // min size
    });
  }

  // in your app create uploader as soon as the DOM is ready
  // don't wait for the window to load
  window.onload = createUploader;
</script>
